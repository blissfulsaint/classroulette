{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "single", { "allowTemplateLiterals": true }]
  },
  "globals": {
    "__dirname": "readonly"
  }
}
